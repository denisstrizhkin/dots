#!/bin/sh

my_git="git clone https://github.com/denisstrizhkin"
alias clone_st="$my_git/st"
alias clone_dwm="$my_git/dwm"
alias clone_dmenu="$my_git/dmenu"
alias clone_tabbed="$my_git/tabbed"
alias install="sudo make clean install"

cd $HOME
mkdir progs
cd progs

clone_st
clone_dwm
clone_dmenu
clone_tabbed

cd st; install; cd ..
cd dwm; install; cd ..
cd tabbed; install; cd ..
cd dmenu; install; cd ..

